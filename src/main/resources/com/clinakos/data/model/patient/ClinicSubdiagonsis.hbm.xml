<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.ClinicSubdiagnosis"
		table="clinic_sub_diagnosis">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="clinicDiagnosisId" type="int">
			<column name="clinic_diagnosis_id" />
		</property>

		<property name="subDiagnosisName">
			<column name="sub_diagnosis_name" />
		</property>

		
	</class>
</hibernate-mapping>