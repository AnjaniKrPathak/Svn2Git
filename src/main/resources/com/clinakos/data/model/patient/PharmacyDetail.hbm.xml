<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.PharmacyDetail"
		table="pharmacy_details">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>

		<property name="patientId" type="int">
			<column name="patient_id" />
		</property>
	
		<property name="drugInfo" type="string">
			<column name="drug_info" />
		</property>
		
		<property name="quantity" type="java.lang.Integer">
			<column name="quantity" />
		</property>
		
		<property name="dateOfPurchase" type="java.util.Date">
			<column name="date_of_purchase" />
		</property>
		
		<property name="ndcCode" type="java.lang.Double">
			<column name="ndc_code" />
		</property>
		
		<property name="drugId" type="java.lang.Double">
			<column name="drug_id" />
		</property>
		
		<property name="pharmacyName" type="string">
			<column name="pharmacy_name" />
		</property>
		
		
		<property name="pharmacyContactNumber" type="string">
			<column name="pharmacy_contact_number" />
		</property>
		
		
		<!-- <property name="drugHistoryTransactionGuid" type="string">
			<column name="drug_history_transaction_guid" />
		</property>
		 -->
		
		<property name="source" type="string">
			<column name="source" />
		</property>
		
		
		<property name="healthplanName" type="string">
			<column name="health_plan_name" />
		</property>
		
		<!-- <property name="daysSupply" type="string">
			<column name="days_supply" />
		</property> -->
		
		
		<property name="doctorName" type="string">
			<column name="doctor_name" />
		</property>
		
		
		<property name="dosage" type="string">
			<column name="dosage" />
		</property>
		
		<property name="downloaddate" type="java.util.Date">
			<column name="download_date" />
		</property>
		
		<property name="drugNameId" type="java.lang.Double">
			<column name="drug_name_id" />
		</property>

		<property name="daysOfSupply" type="java.lang.String">
		<column name="days_supply"></column>
		</property>

	</class>
</hibernate-mapping>