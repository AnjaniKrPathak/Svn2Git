			lazy="true" fetch="select"> <key> <column name="id" not-null="true" /> </key> 
			<one-to-many class="com.clinakos.data.model.patient.ClinicDiagonsisLab" /> 
			</set> -->
		<!-- <bag name="clinicDiagonsisLabList" table="clinic_diagonsis_lab" inverse="true" 
			lazy="true" fetch="select"> <key> <column name="clinic_diagonsis_id" not-null="true" 
			/> </key> <one-to-many class="com.clinakos.data.model.patient.ClinicDiagonsisLab" 
			/> </bag> -->

		<list name="clinicDiagonsisLabList" table="clinic_diagonsis_lab"
			inverse="true" lazy="true" fetch="select" cascade="all">
			<key column="clinic_diagonsis_id" not-null="true" />
			<list-index column="index_col" />

			<one-to-many class="com.clinakos.data.model.patient.ClinicDiagonsisLab" />
		</list>

