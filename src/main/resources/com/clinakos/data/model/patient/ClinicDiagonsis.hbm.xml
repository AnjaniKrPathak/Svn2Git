<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.ClinicDiagonsis"
		table="clinic_diagnosis">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>

		<property name="clinicId" type="int">
			<column name="clinic_id" />
		</property>

		<property name="diagnosisName">
			<column name="diagonsis_name" />
		</property>

		<property name="icdId" type="int">
			<column name="icd_id" />
		</property>


		<property name="medicineName">
			<column name="medicine_name" />
		</property>
