<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.MasterTimeZone" table="master_time_zone">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="timeZone" type="java.lang.String">
			<column name="time_zone" />
		</property>

		<property name="description" type="java.lang.String">
			<column name="description" />
		</property>

		<property name="relativeToGMT" type="java.lang.String">
			<column name="relative_to_GMT" />
		</property>
		

	</class>
</hibernate-mapping>