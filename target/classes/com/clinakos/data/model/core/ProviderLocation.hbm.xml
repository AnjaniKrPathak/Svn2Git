<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.core.ProviderLocation" table="provider_location">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>


		 <property name="providerId" type="int"  insert="false" update="false">
			<column name="provider_id" />
		</property> 
		
		<many-to-one name="providerDetail" class="com.clinakos.data.model.core.ProviderDetail" fetch="select">
            <column name="provider_id" not-null="true" />
        </many-to-one>
		
		<property name="addressLine1" type="java.lang.String">
			<column name="address_line1" />
		</property>

		<property name="addressLine2" type="java.lang.String">
			<column name="address_line2" />
		</property>
		
		<property name="zipCode" type="java.lang.String">
			<column name="zip_Code" />
		</property>
		
		<property name="city" type="java.lang.String">
			<column name="city" />
		</property>
		
		<property name="state" type="java.lang.String">
			<column name="state" />
		</property>
		
		<property name="country" type="java.lang.String">
			<column name="country" />
		</property>
		
		<property name="primaryPhoneNumber" type="java.lang.String">
			<column name="primary_phone_number" />
		</property>
		
		
			<property name="primaryContactNumber" type="java.lang.String">
			<column name="primary_contact_number" />
		</property>
		
		<property name="faxNumber" type="java.lang.String">
			<column name="fax_number" />
		</property>
		
		<property name="location" type="java.lang.String">
			<column name="location_name" />
		</property>
		
		<property name="accountId" type="java.lang.String">
			<column name="account_id" />
		</property>
		
		<property name="siteId" type="java.lang.String">
			<column name="site_id" />
		</property>
		
		<!-- <property name="zip4Code" type="integer">
			<column name="zip4_code" />
		</property>
 -->
	</class>
</hibernate-mapping>