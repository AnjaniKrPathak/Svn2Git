<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.PharmacogenomicsUserSummary"
		table="pharmacogenomics_user_summary">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="userId" type="int">
			<column name="user_id" />
		</property>


		<property name="geneSymbol" type="java.lang.String">
			<column name="gene_symbol" />
		</property>


		<property name="genoType" type="java.lang.String">
			<column name="geno_type" />
		</property>
		<property name="phenoType" type="java.lang.String">
			<column name="pheno_type" />
		</property>
		<property name="attentionRating" type="java.lang.String">
		<column name="attention_rating"></column>
		
		</property>
	
	</class>
</hibernate-mapping>