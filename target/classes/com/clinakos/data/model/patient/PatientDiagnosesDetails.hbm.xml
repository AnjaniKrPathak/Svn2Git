<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.PatientDiagnosesDetails" 
		table="patient_diagnoses_details">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="patientId" type="int">
			<column name="user_id" />
		</property>


		<property name="icdId" type="java.lang.String">
			<column name="icd_id" />
		</property>

		<property name="code" type="java.lang.String">
			<column name="code" />
		</property>


		<property name="codeDescription" type="java.lang.String">
			<column name="code_description" />
		</property>
		<property name="icdCodeType" type="java.lang.String">
			<column name="icd_code_type" />
		</property>

		<property name="date">
			<column name="date" />
		</property>
		<!-- <many-to-one name="doctorDetail" column="doctor_detail_id" class="com.clinakos.data.model.patient.DoctorDetail" 
			not-null="true" /> -->
		<!-- <many-to-one name="userLoginDetail" column="doctor_detail_id"
			class="com.clinakos.data.model.patient.UserLoginDetail"  /> -->
		<property name="doctorId" column="doctor_detail_id" type="int"/>
		
		<property name="ExternalDoctorUpin" type="java.lang.String">
			<column name="external_doctor_upin" />
		</property>
	</class>
</hibernate-mapping>