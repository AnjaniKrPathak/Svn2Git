<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.ClinicDiagonsisLab"
		table="clinic_diagnosis_lab">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<!-- <property name="clinicDiagnosisId" type="int"> <column name="clinic_diagonsis_id" 
			/> </property> -->

		<property name="labName">
			<column name="lab_name" />
		</property>

		<many-to-one name="clinicDiagonsis"
			class="com.clinakos.data.model.patient.ClinicDiagonsis" fetch="select">
			<column name="clinic_diagonsis_id" not-null="true" />
		</many-to-one>

	</class>
</hibernate-mapping>