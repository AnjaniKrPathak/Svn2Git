<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.LOVCode" table="lov_code">
		<id name="lOVCodeId" type="int" column="lov_code_id">
			<generator class="native" />
		</id>

		<many-to-one name="lOVType"
			class="com.clinakos.data.model.patient.LOVType" fetch="select">
			<column name="lov_type_id" />
		</many-to-one>

		<property name="label" type="java.lang.String">
			<column name="label" />
		</property>

		<property name="description" type="java.lang.String">
			<column name="description" />
		</property>



	</class>
</hibernate-mapping>