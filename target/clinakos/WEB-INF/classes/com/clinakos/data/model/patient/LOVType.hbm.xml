<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.LOVType" table="lov_type">
		<id name="id" type="int" column="lov_type_id">
			<generator class="identity" />
		</id>

		<property name="text" type="java.lang.String">
			<column name="text" />
		</property>


		<property name="textDescription" type="java.lang.String">
			<column name="text_description" />
		</property>

		<set name="lovCodeDetail" table="lov_code" inverse="true" lazy="true"
			fetch="select">
			<key>
				<column name="lov_type_id" not-null="true" />
			</key>
			<one-to-many class="com.clinakos.data.model.patient.LOVCode" />
		</set>
		<!-- <list name="lovCodeDetail" table="Lov_Code"> <list-index column="Lov_Type_Id"/> 
			<one-to-many class="com.clinakos.core.model.LOVCode"/> </list> -->
		<!-- <list name="lovCodeDetail" cascade="all"> <key column="Lov_Type_Id"/> 
			<list-index column="LovType_Id"/> <one-to-many class="com.clinakos.core.model.LOVCode"/> 
			</list> -->

		<!-- <list lazy="false" name="lovCodeDetail" table="Lov_Code" cascade="all"> 
			<list-index column="Lov_Type_Id"/> <one-to-many class="com.clinakos.core.model.LOVCode"/> 
			<element></element> </list> -->



	</class>
</hibernate-mapping>