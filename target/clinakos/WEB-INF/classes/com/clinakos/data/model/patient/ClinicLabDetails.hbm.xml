<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.ClinicLabDetails"
		table="clinic_lab_details">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="clinicDiagnosisLabId" type="int">
			<column name="clinic_diagonsis_lab_id" />
		</property>

		<property name="labRange" type="string">
			<column name="lab_range" />
		</property>

		<property name="labNotes">
			<column name="notes" />
		</property>



	</class>
</hibernate-mapping>