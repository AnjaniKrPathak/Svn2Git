<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.core.EmdeonLabResult" table="emdeon_lab_result">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="labResultValue" type="java.lang.String">
			<column name="lab_result_value" />
		</property>


		<property name="refrenceRange" type="java.lang.String">
			<column name="refrence_range" />
		</property>

		<property name="comments" type="java.lang.String">
			<column name="comment" />
		</property>

		<property name="labName" type="java.lang.String">
			<column name="lab_name" />
		</property>
		<property name="labUnit" type="java.lang.String">
			<column name="lab_unit" />
		</property>
		
		<property name="patientId" type="java.lang.String">
			<column name="patient_id" />
		</property>
		<property name="labResultCode" type="java.lang.String">
			<column name="lab_result_code" />
		</property>
		<property name="loincCode" type="java.lang.String">
			<column name="loinc_code" />
		</property>
		<property name="labResultStatus" type="java.lang.String">
			<column name="lab_result_status" />
		</property>



	</class>
</hibernate-mapping>