<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.clinakos.data.model.core.FormularyDetail" table="ext_formulary">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>

		<property name="medicineId" type="int">
			<column name="medicine_id" />
		</property>

		<property name="insuranceId" type="int">
			<column name="insurance_id" />
		</property>

		<property name="formulayTier" type="java.lang.String">
			<column name="formulay_tier" />
		</property>

		<property name="patientCopay" type="java.lang.String">
			<column name="patient_copay" />
		</property>

		<property name="medicineName" type="java.lang.String">
			<column name="medicine_name" />
		</property>

		<property name="therapyType" type="java.lang.String">
			<column name="therapy_type" />
		</property>

	</class>
</hibernate-mapping>