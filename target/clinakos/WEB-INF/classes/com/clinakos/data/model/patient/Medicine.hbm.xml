<hibernate-mapping>
	<class name="com.clinakos.data.model.patient.Medicine" table="medicine_details">
		<id name="id">
			<column name="id" />
			<generator class="increment" />
		</id>

		<property name="medicinName">
			<column name="medicine_name" />
		</property>

		<property name="clinicName">
			<column name="clinic_name" />
		</property>
		<property name="flagForMedActionPlan" type="boolean">
			<column name="flag_for_med_action_plan" />
		</property>
		<property name="flagForDoseCalculator" type="boolean">
			<column name="flag_for_dose_calculator" />
		</property>



	</class>
</hibernate-mapping>
